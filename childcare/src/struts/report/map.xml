<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="report" namespace="/report" extends="web-default">
	
		<action name="frame" class="pdn.sci.cs.action.MonthlyInformationAction" method="frame">
			<result name="success">/WEB-INF/content/monthly/frame.jsp</result>
		</action>
		
		<action name="view" class="pdn.sci.cs.action.MonthlyInformationAction" method="view">
			<result name="success">/WEB-INF/content/monthly/view.jsp</result>
		</action>
		
		<action name="search" class="pdn.sci.cs.action.MonthlyInformationAction" method="search">
			<result name="success">/WEB-INF/content/monthly/view.jsp</result>
		</action>
		
		<action name="add" class="pdn.sci.cs.action.MonthlyInformationAction" method="add">
			<result name="success">/WEB-INF/content/monthly/add.jsp</result>
		</action>
		
		<action name="search-form" class="pdn.sci.cs.action.MonthlyInformationAction" method="searchForm">
			<result name="success">/WEB-INF/content/monthly/search.jsp</result>
		</action>
	
	</package>
	
</struts>