<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="childCareplan" namespace="/childCareplan" extends="web-default">

		<action name="list" class="pdn.sci.cs.action.ChildCareplanAction" method="list">
			<result name="success">/WEB-INF/content/child/care-plan/list.jsp</result>
		</action>

		<action name="frame" class="pdn.sci.cs.action.ChildCareplanAction" method="frame">
			<result name="success">/WEB-INF/content/child/care-plan/frame.jsp</result>
		</action>

		<action name="search-form" class="pdn.sci.cs.action.ChildCareplanAction" method="searchForm">
			<result name="success">/WEB-INF/content/child/care-plan/search.jsp</result>
		</action>

		<action name="search" class="pdn.sci.cs.action.ChildCareplanAction" method="search">
			<result name="success">/WEB-INF/content/child/care-plan/list.jsp</result>
		</action>

		<action name="add" class="pdn.sci.cs.action.ChildCareplanAction" method="add">
			<result name="success">/WEB-INF/content/child/care-plan/add.jsp</result>
		</action>

		<action name="edit" class="pdn.sci.cs.action.ChildCareplanAction" method="edit">
			<result name="success">/WEB-INF/content/child/care-plan/add.jsp</result>
		</action>

		<action name="view" class="pdn.sci.cs.action.ChildCareplanAction" method="view">
			<result name="success">/WEB-INF/content/child/care-plan/view.jsp</result>
		</action>

		<action name="delete" class="pdn.sci.cs.action.ChildCareplanAction" method="delete">
			<result name="success">/WEB-INF/content/child/care-plan/list.jsp</result>
		</action>

		<action name="save" class="pdn.sci.cs.action.ChildCareplanAction" method="save">
			<result name="success">/WEB-INF/content/child/care-plan/view.jsp</result>
			<result name="input">/WEB-INF/content/child/care-plan/add.jsp</result>
		</action>

	</package>
</struts>