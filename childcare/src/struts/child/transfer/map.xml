<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="childTransfer" namespace="/childTransfer" extends="web-default">
	
		<action name="list" class="pdn.sci.cs.action.ChildTransferAction" method="list">
			<result name="success">/WEB-INF/content/child/transfer/list.jsp</result>
		</action>
		
		<action name="frame" class="pdn.sci.cs.action.ChildTransferAction" method="frame">
			<result name="success">/WEB-INF/content/child/transfer/frame.jsp</result>
		</action>
		
		<action name="view" class="pdn.sci.cs.action.ChildTransferAction" method="view">
			<result name="success">/WEB-INF/content/child/transfer/view.jsp</result>
		</action>
		
		<action name="edit" class="pdn.sci.cs.action.ChildTransferAction" method="edit">
			<result name="success">/WEB-INF/content/child/transfer/add.jsp</result>
		</action>
		
		<action name="unconfirmedlist" class="pdn.sci.cs.action.ChildTransferAction" method="unconfirmedList">
			<result name="success">/WEB-INF/content/child/transferUnconfirmList.jsp</result>
		</action>
		
		<action name="confirm" class="pdn.sci.cs.action.ChildTransferAction" method="confirm">
			<result name="success">/WEB-INF/content/child/transferUnconfirmList.jsp</result>
			<result name="input">/WEB-INF/content/child/transferUnconfirmList.jsp</result>
		</action>
		
		<action name="notConfirm" class="pdn.sci.cs.action.ChildTransferAction" method="notConfirm">
			<result name="success">/WEB-INF/content/child/transferUnconfirmList.jsp</result>
			<result name="input">/WEB-INF/content/child/transferUnconfirmList.jsp</result>
		</action>
		
		<action name="delete" class="pdn.sci.cs.action.ChildTransferAction" method="delete">
			<result name="success">/WEB-INF/content/child/transfer/list.jsp</result>
			<result name="success">/WEB-INF/content/child/transfer/list.jsp</result>
		</action>
		
		<action name="add" class="pdn.sci.cs.action.ChildTransferAction" method="add">
			<result name="success">/WEB-INF/content/child/transfer/add.jsp</result>
		</action>
		
		<action name="save" class="pdn.sci.cs.action.ChildTransferAction" method="save">
			<result name="success">/WEB-INF/content/child/transfer/list.jsp</result>
			<result name="input">/WEB-INF/content/child/transfer/add.jsp</result>
		</action>

	</package>
</struts>