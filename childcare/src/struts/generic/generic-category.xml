<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="genericCategory" namespace="/genericCategory" extends="pgis-default">
	
		<action name="frame" class="pdn.sci.cs.action.GenericCategoryAction" method="viewFrame">
			<result name="success">/WEB-INF/content/settings/generic/category/frame.jsp</result>
		</action>
		
		<action name="list" class="pdn.sci.cs.action.GenericCategoryAction" method="list">
			<result name="success">/WEB-INF/content/settings/generic/category/list.jsp</result>
		</action>
		
		<action name="view" class="pdn.sci.cs.action.GenericCategoryAction" method="view">
			<result name="success">/WEB-INF/content/settings/generic/category/view.jsp</result>
		</action>
		
		<action name="add" class="pdn.sci.cs.action.GenericCategoryAction" method="add">
			<result name="success">/WEB-INF/content/settings/generic/category/add.jsp</result>
		</action>
		
		<action name="edit" class="pdn.sci.cs.action.GenericCategoryAction" method="edit">
			<result name="success">/WEB-INF/content/settings/generic/category/add.jsp</result>
			<result name="input">/WEB-INF/content/error.jsp</result>
		</action>
		
		<action name="delete" class="pdn.sci.cs.action.GenericCategoryAction" method="delete">
			<result name="success">/WEB-INF/content/settings/generic/category/list.jsp</result>
		</action>
		
		<action name="save" class="pdn.sci.cs.action.GenericCategoryAction" method="save">
			<result name="success">/WEB-INF/content/settings/generic/category/view.jsp</result>
			<result name="input">/WEB-INF/content/settings/generic/category/add.jsp</result>
		</action>

	</package>
</struts>